{
	"name": "app-quickstart",
	"type": "module",
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"predev": "node -e \"require('node:fs').mkdirSync('logs', { recursive: true })\"",
		"dev": "astro dev 2>&1 | tee logs/dev.log",
		"build": "astro build",
		"preview": "astro preview",
		"prepare": "if [ -z \"$CI\" ]; then husky; fi",
		"test": "vitest --no-watch",
		"test:unit": "vitest --no-watch -t 'unit'",
		"test:integration": "vitest --no-watch",
		"check:ts": "tsc --noEmit",
		"check:biome": "biome check --write --no-errors-on-unmatched",
		"fix": "npm run check:biome && npm run check:ts && npm run test:integration",
		"db:start": "supabase start",
		"db:stop": "supabase stop",
		"db:reset": "supabase db reset",
		"db:generate-seed": "tsx scripts/generate-seed.ts"
	},
	"lint-staged": {
		"**/*.{ts,tsx,json,css,md,astro,html}": [
			"biome check --write --no-errors-on-unmatched"
		]
	},
	"dependencies": {
		"@astrojs/vercel": "^9.0.2",
		"@heroicons/vue": "^2.2.0",
		"@supabase/supabase-js": "^2.90.0",
		"@vueuse/core": "^13.9.0",
		"fuse.js": "^7.1.0",
		"libphonenumber-js": "^1.12.24",
		"resend": "^6.6.0",
		"twilio": "^5.10.3",
		"vue": "^3.5.22"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.6",
		"@astrojs/sitemap": "^3.6.0",
		"@astrojs/ts-plugin": "^1.10.6",
		"@astrojs/vue": "^5.1.3",
		"@biomejs/biome": "2.2.6",
		"@tailwindcss/vite": "^4.1.14",
		"@types/pg": "^8.16.0",
		"astro": "^5.16.6",
		"dotenv": "^17.2.3",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.4",
		"pg": "^8.16.3",
		"supabase": "^2.70.5",
		"tailwindcss": "^4.1.18",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"vitest": "^3.1.4"
	}
}
